#PF-Assgn-54
def check_anagram(data1,data2):
    #start writing your code here
    data1 = data1.upper()
    data2 = data2.upper()
    if(sorted(data1)== sorted(data2)):
        for i,j in zip(data1, data2):
            if i == j:
                return False
        return True 
    else: 
        return False
    

print(check_anagram("Reductions","discounter"))
